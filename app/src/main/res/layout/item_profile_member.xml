<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_state_profrfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:fontFamily="@font/noto_medium"
        android:includeFontPadding="false"
        android:textSize="12sp"
        android:textColor="@color/text_grey"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="24dp" />


    <TextView
        android:id="@+id/tv_content_profile"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="24dp"
        android:layout_marginTop="16dp"
        android:ellipsize="end"
        android:fontFamily="@font/noto_medium"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:text=""
        android:textColor="@color/six_black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_state_profrfile" />


    <TextView
        android:id="@+id/tv_type_profile"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/noto_regular"
        android:includeFontPadding="false"
        android:text=""
        android:textColor="@color/text_grey"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_content_profile"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="24dp" />

    <TextView
        android:id="@+id/tv_field_profile"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/noto_regular"
        android:includeFontPadding="false"
        android:text=""
        android:textColor="@color/text_grey"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_type_profile"
        app:layout_constraintTop_toBottomOf="@+id/tv_content_profile"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/tv_member_profile"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/noto_regular"
        android:includeFontPadding="false"
        android:text=""
        android:textColor="@color/text_grey"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_field_profile"
        app:layout_constraintTop_toBottomOf="@+id/tv_content_profile"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="24dp" />


    <include
        layout="@layout/line_gray"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_position_projectfind"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>